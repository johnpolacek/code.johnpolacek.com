package com.johnpolacek.animation {		import flash.display.Sprite;	import flash.events.Event;	import com.johnpolacek.shapes.EllipseShape;		public class PreloadAnimationCircles extends PreloadAnimationSquares {				public function PreloadAnimationCircles(s:int = 8, c:uint = 0x666666, n:int = 6, sp:int = 20, l:Number = 1)		{			super(s,c,n,sp,l);		}				override public function createShapes():void		{			for (var i:int = 0; i < numShapes; i++)			{				var shape:EllipseShape = new EllipseShape(size, size, color);				shape.alignToCenter();				shape.x = i * spacing;				addChild(shape);			}		}	}}