package com.johnpolacek.events {	import flash.events.Event;	/** * TrackEvent is used to dispatch google analytics tracking info *  * @version 15 Jun 2010 * @author John Polacek, john@johnpolacek.com */	 	public class TrackEvent extends Event {		public var trackAction:String;		public var trackLabel:String;		public static const TRACK:String = "TRACK";		public function TrackEvent(type:String, action:String, label:String, bubbles:Boolean = true, cancelable:Boolean = false) {			super(type, bubbles,cancelable);			trackAction = action;			trackLabel = label;		}				public override function clone():Event		{			return new TrackEvent(type, this.trackAction, this.trackLabel, bubbles, cancelable);		}			}}